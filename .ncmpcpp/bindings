def_key "+"
    show_clock
def_key "="
    volume_up

def_key "j"
    scroll_down
def_key "k"
    scroll_up

def_key "ctrl-u"
    page_up
def_key "ctrl-d"
    page_down

def_key "h"
    previous_column
def_key "l"
    next_column

def_key "."
    show_lyrics

def_key "n"
    next_found_item
def_key "N"
    previous_found_item

def_key "d"
    delete_playlist_items
def_key "d"
    delete_browser_items
def_key "d"
    delete_stored_playlist

def_key "A"
    set_volume

# HACK: sloppy fix for bug in mopidy-gmusic
def_key ">"
    next
    next
    previous
def_key "<"
    previous
    previous
    next
def_key "enter"
    enter_directory
def_key "enter"
    toggle_output
def_key "enter"
    run_action
def_key "enter"
    stop
    play_item

